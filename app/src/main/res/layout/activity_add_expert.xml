            android:layout_height="480dp"
            android:layout_marginStart="16dp"
            android:layout_marginLeft="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView_cart"
                android:layout_width="141dp"
                android:layout_height="45dp"
                android:text="@string/slabel_1"
                android:textColor="@color/Mtopic1color"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.229"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.087" />

            <EditText
                android:id="@+id/editexpertName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="@string/sedit_1"

                android:inputType="textPersonName"
                android:textColor="@color/MButton3c"
                android:textColorHint="@color/MButton3c"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.358"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.192" />

            <TextView
                android:id="@+id/textView61"
                android:layout_width="140dp"
                android:layout_height="43dp"
                android:text="@string/slabel_2"
                android:textColor="@color/Mtopic1color"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.228"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.299" />

            <TextView
                android:id="@+id/textView62"
                android:layout_width="142dp"
                android:layout_height="39dp"
                android:text="@string/sedit_3"
                android:textColor="@color/Mtopic1color"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.23"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.482" />

            <EditText
                android:id="@+id/editexpertCarrier"
                android:layout_width="239dp"
                android:layout_height="46dp"
                android:ems="10"
                android:hint="@string/sedit_3"
                android:inputType="textPersonName"
                android:textColor="@color/MButton3c"
                android:textColorHint="@color/MButton3c"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.38"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.58" />

            <TextView
                android:id="@+id/textView63"
                android:layout_width="141dp"
                android:layout_height="40dp"
                android:layout_marginStart="56dp"
                android:layout_marginLeft="56dp"
                android:text="@string/slabel_4"
                android:textColor="@color/Mtopic1color"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.677" />

            <EditText
                android:id="@+id/editexpertWorks"
                android:layout_width="236dp"
                android:layout_height="42dp"
                android:ems="10"
                android:hint="@string/sedit_4"
                android:inputType="textPersonName"
                android:textColor="@color/MButton3c"
                android:textColorHint="@color/MButton3c"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.366"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.771" />

            <ImageView
                android:id="@+id/editcameraImage"
                android:layout_width="100dp"
                android:layout_height="75dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.055"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.96"
                app:srcCompat="@drawable/icons8_image_256" />

            <EditText
                android:id="@+id/editexpertEmail"
                android:layout_width="239dp"
                android:layout_height="46dp"
                android:ems="10"
                android:hint="@string/sedit_2"
                android:inputType="textEmailAddress"
                android:textColor="@color/MButton3c"
                android:textColorHint="@color/MButton3c"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.38"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.384" />

            <android.widget.Button
                android:id="@+id/buttonCamera"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/Mtopic1color"
                android:text="Take a photo"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.9" />

            <Button
                android:id="@+id/listExpertBtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/list_all"
                app:backgroundTint="@color/Mtext3c"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.974"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.042" />

            <android.widget.Button
                android:id="@+id/saveExpert"
                android:layout_width="127dp"
                android:layout_height="37dp"
                android:layout_marginTop="54dp"
                android:background="@color/Mtopic1color"
                android:text="@string/sbtn_1"
                android:textColor="@color/white"
                android:textColorHint="@color/white"
                app:backgroundTint="#ED2ACA30"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/buttonCamera"
                app:layout_constraintVertical_bias="0.0" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>